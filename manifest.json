{
	//Required
	"manifest_version": 2,
	"name": "Currency transformer",
	"version": "0.0.5.4",
	//End required

	"description":"This is just me trying to make a chrome extension",
	"icons": { "16": "icons/16x16.png", "48": "icons/48x48.png", "128": "icons/128x128.png" },

	"author":"b to the v",

	"content_scripts": [
	    {
	      "matches": ["http://*/*"],
	      "js": ["contentscript.js"],
